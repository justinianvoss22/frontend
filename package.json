{
  "name": "frontend",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:integration": "npx playwright test --config tests/integration",
    "test:e2e": "npx playwright test --config tests/e2e",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e --skip-build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "format:check": "prettier --plugin-search-dir=. . --check"
  },
  "engines": {
    "node": "^18.14.0"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^2.1.0",
    "@playwright/test": "^1.37.1",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.27.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/svelte": "^4.0.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3-time": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cryptocurrency-icons": "^0.18.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.33.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "postcss-dark-theme-class": "^1.0.0",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-simple-json-server": "^0.6.2",
    "vitest": "^0.34.6",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "dependencies": {
    "@aave/math-utils": "^1.23.1",
    "@sentry/sveltekit": "^7.77.0",
    "@trading-strategy-ai/web-top-node": "^0.1.0-beta.10",
    "@tryghost/content-api": "^1.11.8",
    "@types/d3-array": "^3.2.0",
    "@wagmi/connectors": "^4.1.10",
    "@wagmi/core": "^2.6.1",
    "bignumber.js": "^9.1.2",
    "cheerio": "^1.0.0-rc.12",
    "cookie": "^0.5.0",
    "d3-array": "^3.2.4",
    "d3-time": "^3.1.0",
    "date-fns": "^2.29.3",
    "dd-trace": "^4.15.0",
    "devalue": "^4.3.2",
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "svelte-fsm": "^1.2.0",
    "svelte-headless-table": "^0.17.5",
    "svelte-highlight": "^7.3.0",
    "svelte-inview": "^4.0.2",
    "svelte-spinner": "^2.0.2",
    "typesense": "^1.7.1",
